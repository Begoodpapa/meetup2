<form id="login_form" action="/auth/login" method="post">
  <div class="form-group">
    <label for="uid">User name:</label>
    <input type="text" id="uid" name="uid" class="form-control input-lg" placeholder="Nokia Intra Name (like g12wang...)" required="required">
  </div>
  <div class="form-group">
    <label for="password">Password:</label>
    <input type="password" class="form-control input-lg" id="password" name="password" placeholder="Password" required="required">
  </div>
  <div class="checkbox">
    <label>
      <input type="checkbox" id="remember_me"> Remember me
    </label>
  </div>
  <button id="login_btn" type="submit" data-loading-text="Log In..." class="btn btn-primary btn-lg btn-block" autocomplete="off">Log In</button>
</form>
