
<div class="container">

<h3><span class="label label-default">Day 1</span></h3>

<div class="row">

<% _.each(day1topics, function(topic, index){ %>

  <% if(index%4===0 && index>0) {%>
    </div>
    <div class="row">
  <% } %>

  <div class="col-sm-3 col-md-3">
    <br/>
    <p>
    <div class="thumbnail">
      <img src=<%=topic.projectImage%> alt="...">
      <h4 class="speaker"><strong><%= topic.speaker %></strong></h4>
      <h4 class="speakerinfo"><%= topic.info %></h4>
      <h5 class="topicname"><%= topic.name %></h5>
    </div>
  </div>

<% }) %>

</div>

