<% }) %>

</div>

<h3><span class="label label-default">Agenda on June.22</span></h3>


<% _.each(day1Agendas, function(agenda, index){ %>
  
  <div class="panel panel-default">
    <!-- Default panel contents -->
    <div class="panel-heading"><%= agenda.name %></div>
    <div class="panel-body">
      <p>Address: <span style="color: #c7551c; fond-size:18px"><%=agenda.address %></span></p>
    </div>

    <!-- Table -->
    <table class="table">
      <thead>
        <tr>
          <th>Topic</th>
          <th>Speaker</th>
          <th>When</th>
        </tr>
      </thead>
      <tbody>
        <% _.each(agenda.topics, function(topic, index){ %>
          <tr>
            <td style="width:600px"><%= topic.name %></td>
            <td><strong><%= topic.speaker %></strong></td>
            <td><%= topic.when %></td>
          </tr>
        <% }) %>
      </tbody>
    </table>
  </div>

<% }) %>

